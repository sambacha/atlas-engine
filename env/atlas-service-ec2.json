        "InternalAlbListener" : {
            "Type" : "AWS::ElasticLoadBalancingV2::Listener",
            "DependsOn" : "InfoSvcTargetGroup",
            "Properties" : {
                "LoadBalancerArn" : { 
                    "Fn::ImportValue" :  {
                        "Fn::Sub": "AtlasInternalAlbArn" 
                    } 
                },
                "DefaultActions" : [ {
                    "ForwardConfig" : {
                        "TargetGroups" : [ {
                            "TargetGroupArn" : {
                                "Ref" : "InfoSvcTargetGroup"
                            }
                        } ]
                    },
                    "Order" : 100,
                    "TargetGroupArn" : {
                        "Ref" : "InfoSvcTargetGroup"
                    },
                    "Type" : "forward"
                }],
                "Port" : 80,
                "Protocol" : "HTTP"
            }
        },
        "InternalAlbListnerProductInternalApiRule" : {
            "Type" : "AWS::ElasticLoadBalancingV2::ListenerRule",
            "DependsOn" : "InternalAlbListener",
            "Properties" : {
                "Actions" : [ {
                    "ForwardConfig" : {
                        "TargetGroups" : [ {
                            "TargetGroupArn" : {
                                "Ref" : "ProductSvcTargetGroup"
                            }
                        } ]
                    },
                    "Type" : "forward"
                } ],
                "Conditions" : [ {
                    "Field" : "path-pattern",
                    "PathPatternConfig" : {
                        "Values" : [
                            "/atlas/internal/api/product/*"
                        ]
                    }
                } ],
                "ListenerArn" : {
                    "Ref" : "InternalAlbListener"
                },
                "Priority" : 1
            }
        },
        "InternalAlbListnerProductExternalApiRule" : {
            "Type" : "AWS::ElasticLoadBalancingV2::ListenerRule",
            "DependsOn" : "InternalAlbListener",
            "Properties" : {
