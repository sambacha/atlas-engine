                        "TargetGroups" : [ {
                            "TargetGroupArn" : {
                                "Ref" : "ProductSvcTargetGroup"
                            }
                        } ]
                    },
                    "Type" : "forward"
                }],
                "Conditions" : [ {
                    "Field" : "path-pattern",
                    "PathPatternConfig" : {
                        "Values" : [
                            "/atlas/api/product/*"
                        ]
                    }
                } ],
                "ListenerArn" : {
                    "Ref" : "InternalAlbListener"
                },
                "Priority" : 2
            }
        },
